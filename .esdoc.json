{
  "source": "./",
  "destination": "./docs",
  "index": "./README.md",
  "includes": [
    "\\.js$",
    "\\.ts$"
  ],
  "excludes": [ 
    "\\.json$",
    "eslintrc.js",
    "\\.md$",
    "node_modules"
  ],
  "plugins": [
    {
      "name": "esdoc-standard-plugin"
    },
    {
      "name": "esdoc-typescript-plugin"
    }
  ]
}
 