{
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "removeComments": false,
    "declaration": true,
    "sourceMap": true,

    "strict": true,
    "noImplicitAny": false,

    "baseUrl": ".",
    "outDir": "./lib",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": [
    "src",
    "util"
  ],
  "typedocOptions":{
    "entryPoints": [
      "util"
    ],
    "exclude": [
      "node_moudles"
    ],
    "includeVersion": true,
    "name": "测试title",
    "hideGenerator": true,
    "disableSources": true
  }
}